{
  "name": "Header Facebook Fixed",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fixed header Facebook at the top",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  },
  "permissions": [
    "http://*.facebook.com/",
    "*://*.facebook.com/",
  ],
  "content_scripts": [{
    "matches": [ "*://facebook.com/*" ],
    "js": [ "header-facebook-fixed.js" ]
    "js": [ "jquery-1.11.3.min.js", "header-facebook-fixed.js" ]
  }]
}
